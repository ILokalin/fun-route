{
  "name": "fun-route",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.16.3"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
