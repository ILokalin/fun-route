    "puppeteer-io": "^1.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.11.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:all": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "npm run test:all -- --t='E2E' --watchAll=false",
    "test": "npm run test:all -- --t='unit' --watchAll=false --reporters jest-silent-reporter"
